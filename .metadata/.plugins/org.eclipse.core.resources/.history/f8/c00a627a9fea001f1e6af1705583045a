/* Private typedef -----------------------------------------------------------*/
/* Private define ------------------------------------------------------------*/
/* Private macro -------------------------------------------------------------*/
/* Private variables ---------------------------------------------------------*/
/* Private function prototypes -----------------------------------------------*/
const char* event_type_2_str(event_type_t evt)
{
    switch (evt)
    {
    	case_str(EVT_none)
		case_str(EVT_Remote_rs232)
		case_str(EVT_Remote_Ether)
		case_str(EVT_StopRemote)
    	default :
			break;
    }

    return "Unknown event";
}

